{
  "name": "philme-in",
  "version": "1.4.0",
  "main": "index.html",
  "author": "Philipp Wille",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "start": "http-server",
    "dist": "webpack --progress --colors --config webpack.config.js",
    "dev": "webpack-dev-server --progress --colors --config webpack.config.js --port 9411",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "jest --coverage --watch",
    "code-quality": "plato --noempty --recurse --dir report --exclude ^.*\\.\\(raw\\|test\\)\\..*$ --eslint ./node_modules/standard/eslintrc.json src"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ],
      "react"
    ]
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/app.js",
      "!src/config/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "http-server": "^0.10.0",
    "jest": "^20.0.4",
    "plato": "^1.7.0",
    "react-styleguidist": "^6.0.15",
    "react-test-renderer": "^15.6.1",
    "standard": "^10.0.2",
    "standard-loader": "^6.0.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "prop-types": "^15.5.10",
    "ramda": "^0.23.0",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-flip-move": "^2.9.14",
    "react-ga": "^2.2.0",
    "react-redux": "^5.0.5",
    "react-syntax-highlighter": "^5.6.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "redux-saga-router": "^2.1.0",
    "styled-components": "^2.1.2",
    "uuid": "^3.1.0"
  }
}
